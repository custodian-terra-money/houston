{
  "name": "@terra-money/houston-init",
  "version": "0.2.1",
  "description": "houston project initialization functionality",
  "license": "MIT",
  "author": "Hyungsuk Kang <hskang9@gmail.com>",
  "homepage": "https://github.com/terra-project/houston/tree/master/packages/init#readme",
  "repository": "https://github.com/terra-project/houston/tree/master/packages/init",
  "bugs": {
    "url": "https://github.com/terra-project/houston/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && npm run global",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "mocha src/**/*.spec.ts",
    "lint": "tslint -p . --fix",
    "build": "npm run clean:some && tsc -p .",
    "prepare": "yarn build",
    "clean:some": "rm -rf ./dist",
    "clean:all": "rm -rf ./node_modules ./package-lock.json ./lib ./docs",
    "refresh": "npm run clean:all && npm install",
    "tscov": "tscov --min-coverage 90",
    "tscov:d": "tscov --details",
    "publish-package": "npm run build && npm publish",
    "docs": "typedoc --out docs ./src"
  },
  "keywords": [
    "terra",
    "houston",
    "init"
  ],
  "dependencies": {
    "@terra-money/houston-config": "0.1.8",
    "@types/node": "^14.10.0",
    "find-up": "^4.1.0",
    "json-schema-class": "0.0.1",
    "lodash": "^4.17.19",
    "ora": "^5.0.0",
    "original-require": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "tslint": "^5.12.1",
    "typescript": "^3.9.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "3b571d9da1d4b708e81f3063c499f49ea1ab7c9f"
}
